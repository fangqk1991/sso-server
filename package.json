{
  "name": "@fangcha/sso-server",
  "version": "0.13.0",
  "description": "SSO Server.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "ts-node-dev --no-notify --ignore-watch node_modules demo/apps/sso-web.ts",
    "start:typecheck": "tsc -w --noEmit",
    "test": "nyc mocha --require ts-node/register tests/**/*.test.ts --timeout 1000000 --exit",
    "build": "rm -rf ./lib/ && tsc",
    "rebuild-models": "ts-node ./tools/rebuild-models.ts"
  },
  "repository": "https://github.com/fangqk1991/sso-server.git",
  "keywords": [
    "tools"
  ],
  "author": "fang <me@fangqk.com>",
  "license": "MIT",
  "dependencies": {
    "oauth2-server": "^3.1.1"
  },
  "devDependencies": {
    "@fangcha/account": "^0.9.0",
    "@fangcha/app-error": "^1.5.0",
    "@fangcha/app-request": "^1.6.0",
    "@fangcha/assert": "^1.1.0",
    "@fangcha/backend-kit": "^0.36.0",
    "@fangcha/eslint": "^0.3.2",
    "@fangcha/generator": "^0.1.1",
    "@fangcha/logger": "^0.4.0",
    "@fangcha/router": "^0.18.1",
    "@fangcha/swagger": "^0.1.1",
    "@fangcha/tools": "^1.22.0",
    "@fangcha/webpack": "^0.9.1",
    "@koa/router": "^12.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/koa": "2.13.5",
    "@types/oauth2-server": "^3.0.13",
    "fc-config": "^0.10.0",
    "fc-feed": "^0.19.0",
    "fc-model": "^0.5.0",
    "fc-sql": "^0.29.0",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^5.0.0",
    "log4js": "^6.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.2"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "files": [
    "lib/*",
    "src/*"
  ],
  "nyc": {
    "sourceMap": false
  }
}
